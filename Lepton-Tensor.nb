 "   ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["L", 
    RowBox[{"\[Mu]\[Nu]", " "}]], TraditionalForm]],ExpressionUUID->
  "8a90e6c9-1360-4edb-8ede-89b2e8ad5dc4"],
 "\[Delta] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SubscriptBox["E", "+"]}], TraditionalForm]],ExpressionUUID->
  "e5ae3f15-eb8e-4944-b1be-4c89b1aa7a88"],
 "- (\[Gamma] ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SubscriptBox["m", "*"], "2"], TraditionalForm]],ExpressionUUID->
  "5e4c0f5a-f94e-40be-97dd-65b8ba660ad9"],
